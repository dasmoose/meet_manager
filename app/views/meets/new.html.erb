<div class="row">
  <div class="twelve columns">
    <h1>Create a New Meet</h1> 
    <div class="panel">
      <h3>Meet Details</h3>
        <%= form_for(@meet) do |f| %>
          <label>Name</label>
          <%= f.text_field :name %>
          <label>Location</label>
          <%= f.text_field :location %>
          <label>Number of Lanes in Pool</label>
          <%= f.text_field :pool_size %>
          <%= f.submit "Create Events" %>
        <% end %>
      <p>
      </p>
    </div>
  </div>
</div>
